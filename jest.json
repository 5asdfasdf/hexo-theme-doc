{
  "verbose": true,
  "clearMocks": true,
  "testRegex": "(/__tests__/.*\\.(test|spec))\\.(js|jsx)$",
  "collectCoverageFrom": [
    "!**/__tests__/*.{js,jsx}",
    "lib/browser/**/*.{js,jsx}",
    "lib/nodejs/**/*.js",
    "!lib/browser/sidebar/index.js"
  ],
  "testResultsProcessor": "./node_modules/jest-junit",
  "coverageDirectory": "target/coverage",
  "coverageReporters": [
    "json",
    "lcov",
    "html",
    "cobertura"
  ]
}
