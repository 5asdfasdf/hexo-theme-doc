{
  "verbose": true,
  "clearMocks": true,
  "testRegex": "(/__tests__/.*\\.(test|spec))\\.js?$",
  "testEnvironment": "node",
  "collectCoverageFrom": [
    "!**/__tests__/*.js",
    "lib/browser/**/*.js",
    "lib/nodejs/**/*.js",
    "!lib/browser/sidebar/index.js"
  ],
  "testResultsProcessor": "./node_modules/jest-junit",
  "coverageDirectory": "target/coverage",
  "coverageReporters": [
    "json",
    "lcov",
    "html",
    "cobertura"
  ]
}
