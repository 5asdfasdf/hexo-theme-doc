<% body.forEach((bodyObject) => {
  const meta = bodyObject['__meta'];
  delete bodyObject['__meta'];
%>
  <%=_ `**${meta['name']}**` %>
  <% if(meta['required']){ %>
    <%=_  `* required` %>
  <% } %>
<%
  Object.keys(bodyObject).forEach((key) => {
    const value = bodyObject[key];
%>
    <%=_ `**${key}**` %>
    <% if(value['type']){ %>
      <%=_  `* ${value['type']}` %>
    <% } %>
    <% if(value['required']){ %>
      <%=_  `* required` %>
    <% } %>
    <% if(value['description']){ %>
      <%=_  `* ${value['description']}` %>
    <% } %>
    <% if(value['values']){ %>
      <%=_  '* Allowed values: `' + value['values'].join('`, `') + '`.' %>
    <% } %>
<%
  });
});
%>
