**Parameters**
<% if( request['header'].length ) {%>
  <%=_ `**Header**` %>
  <%- include_module('header', { header: request['header'] }) %>
<% } %>


<% if( request['path'].length || request['query'].length ) {%>
  <%=_ `**Path**` %>
  <%- include_module('requestParams', { params: request['path'].concat(request['query']) }) %>
<% } %>

<% if( request['formData'].length || request['body'].length) {%>
  <%=_ `**Request Body**` %>
  <%- include_module('requestParams', { params: request['formData'] }) %>

  <%- include_module('requestBody', { body: request['body'] }) %>
<% } %>
