<% if(response['description']){ %>
  <%=_  `* ${response['description']}` %>
<% } %>
<% if(response['headers']){ %>
  <%=_ `**Headers**` %>
  <%
    const headers = response['headers'];
    Object.keys(headers).forEach((key) => {
      const header = headers[key];
  %>
      <%=_ `${key}` %>
      <% if(header['description']){  %>
        <%=_ `* ${header['description']}`  %>
      <% } %>
      <% if(header['type']){  %>
        <%=_ `* ${header['type']}`  %>
      <% } %>
      <% if(header['format']){  %>
        <%=_ `* ${header['format']}`  %>
      <% } %>
  <% }) %>
<% } %>

