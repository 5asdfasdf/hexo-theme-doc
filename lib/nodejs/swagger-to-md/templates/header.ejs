<% header.forEach((param) => { %>
  <%=_ `* **${param['name']}**` %>
  <%=_ `* ${param['type']}` %>
  <% if ( param['required'] ) {%>
    <%=_ `* required` %>
  <% } %>
  <%=_ `* ${param['description']}` %>
  <% if ( param['example'] ) { %>
    <%=_ `* Eg: ${param['example']}` %>
  <% } %>
  <%=_ `---` %>
<% }) %>
