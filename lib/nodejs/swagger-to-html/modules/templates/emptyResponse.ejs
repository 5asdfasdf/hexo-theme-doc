<% if(response.headers){ %>
  <div class="card-wrap">
    <h3>Headers</h3>
    <%
      const headers = response.headers;
      Object.keys(headers).forEach((key) => {
        const header = headers[key];
    %>
      <div class="card">
        <div class="main">
          <div class="name">
            <%= key %>
          </div>
          <% if(header.type){  %>
            <div class="type">
              <%=  header.type %> <% if(header.format){ %> (<%= header.format %>) <% } %>
            </div>
          <% } %>
        </div>
        <% if(header.description){  %>
          <div class="description">
            <%= header.description  %>
          </div>
        <% } %>
        <% if(header.format){  %>
          <div class="format">
            <%= header.format  %>
          </div>
        <% } %>
      </div>
    <% }) %>
  </div>
<% } %>
