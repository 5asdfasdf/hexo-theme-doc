{
  "name": "hexo-stargate-doc-theme",
  "version": "0.1.0-alpha",
  "description": "Hexo documentation theme with love by Team Stargate",
  "main": "index.js",
  "bin": {
    "hexo-crawling-test": "./packages/crawling-test/bin/index.js"
  },
  "scripts": {
    "test": "npm run jest",
    "jest": "NODE_ENV=test jest --coverage --no-cache --config ./jest.json",
    "lint": "eslint lib packages scripts --ext .js,.jsx",
    "lint:report": "npm run lint -- -f checkstyle --output-file target/checkstyle.xml",
    "compile:sass": "node-sass source/style/_dress-code.scss source/style/dress-code.css",
    "compile:js": "webpack",
    "compile": "npm run compile:sass && npm run compile:js",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "prerelease": "npm run lint && npm run test && npm run compile && npm run changelog"
  },
  "repository": {
    "type": "git",
    "url": "git@github.bus.zalan.do:stargate/hexo-stargate-doc-theme.git"
  },
  "keywords": [
    "hexo",
    "documentation",
    "theme"
  ],
  "author": "Team Stargate <team-stargate@zalando.de>",
  "license": "MIT",
  "dependencies": {
    "cheerio": "^1.0.0-rc.1",
    "consolidate": "^0.14.5",
    "ejs": "git+https://git@github.com/bhaskarmelkani/ejs.git",
    "escape-string-regexp": "^1.0.5",
    "hexo-log": "^0.1.3",
    "js-crawler": "^0.3.19",
    "js-yaml": "^3.8.4",
    "json-markup": "^1.1.0",
    "jsx-loader": "^0.13.2",
    "lodash": "^4.17.4",
    "lunr": "2.1.0",
    "node-jsx": "^0.13.3",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "striptags": "^3.0.1",
    "swagger-parser": "^3.4.1",
    "touch": "^1.0.0",
    "url-join": "^2.0.2"
  },
  "devDependencies": {
    "conventional-changelog-cli": "^1.3.3",
    "dress-code": "^2.1.1",
    "eslint": "^4.1.1",
    "eslint-plugin-jest": "^20.0.3",
    "eslint-plugin-react": "^7.2.1",
    "hexo": "^3.3.7",
    "hexo-renderer-ejs": "^0.3.0",
    "jest": "^20.0.4",
    "jest-junit": "^2.1.0",
    "jquery": "^3.2.1",
    "node-sass": "^4.5.3",
    "jsx-loader": "^0.13.2",
    "webpack": "^3.0.0"
  },
  "jest-junit": {
    "output": "./target/junit.xml"
  }
}
